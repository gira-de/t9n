{
  "name": "@gira-de/t9n-svelte",
  "version": "2.0.0",
  "author": "GIRA",
  "contributors": [
    "Christian Krey <christian@krey.io>",
    "Nico Beierle <nico.beierle@bztes.de>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gira-de/t9n.git"
  },
  "keywords": [
    "t9n",
    "translation",
    "i18n",
    "svelte"
  ],
  "bugs": {
    "url": "https://github.com/gira-de/t9n/issues"
  },
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js"
    }
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "svelte": "^3.46.4",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.0"
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:umd",
    "build:esm": "tsc -P ./tsconfig.json",
    "build:umd": "rollup lib/index.js --format umd --sourcemap -o lib/index.umd.js --name 't9n'",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest ./src"
  },
  "files": [
    "./lib/",
    "./types/",
    "./package.json",
    "./README.md"
  ]
}
