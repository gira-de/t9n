{
  "name": "@gira-de/t9n-svelte",
  "version": "2.0.1",
  "author": "GIRA",
  "contributors": [
    "Christian Krey <christian@krey.io>",
    "Nico Beierle <nico.beierle@bztes.de>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gira-de/t9n.git"
  },
  "keywords": [
    "t9n",
    "translation",
    "i18n",
    "svelte"
  ],
  "bugs": {
    "url": "https://github.com/gira-de/t9n/issues"
  },
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./lib/index.umd.js"
      }
    }
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.5",
    "rollup": "^4.14.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "svelte": "^4.2.12",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:umd",
    "build:esm": "tsc -P ./tsconfig.json",
    "build:umd": "rollup lib/index.js --format umd --sourcemap -o lib/index.umd.js --name 't9n'",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest ./src"
  },
  "files": [
    "./lib/",
    "./types/",
    "./package.json",
    "./README.md"
  ]
}
